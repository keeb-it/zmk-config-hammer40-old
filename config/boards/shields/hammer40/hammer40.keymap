#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
              &kp A      &kp B        &kp C
              &bt BT_CLR &bt BT_SEL 0 &bt BT_SEL 1
              &kp N1     &kp N2       &kp N3
            >;

            sensor-bindings = <&inc_dec_kp C_VOL_DN C_VOL_UP>;
        };
    };
};
